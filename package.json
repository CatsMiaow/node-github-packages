{
  "name": "@ownername/module-name",
  "version": "0.0.0",
  "description": "Github Packages Template for Node.js",
  "main": "dist",
  "types": "dist",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint --ext .ts .",
    "build": "del-cli dist && tsc -p tsconfig.build.json",
